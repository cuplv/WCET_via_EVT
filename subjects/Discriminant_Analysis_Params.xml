<?xml version="1.0"?>
 <inputs>
  <!--Linear vs Quadratic Discriminant Analysis -->
  <param0>
    <name>linear(0)_quadratic(1)</name>
    <type>bool</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
  </param0>
  <!--solver_Linear-->
  <param1>
    <name>solver_Linear</name>
    <type>int</type>
    <min>0</min>
    <max>2</max>
    <def>0</def>
    <valtrans>svd</valtrans>
    <valtrans>lsqr</valtrans>
    <valtrans>eigen</valtrans>
  </param1>
  <!--Shrinkage Linear-->
  <param2>
    <name>Shrinkage_Linear</name>
    <type>int</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
    <valtrans>None</valtrans>
    <valtrans>auto</valtrans>
    <valtrans>float</valtrans>
  </param2>
  <!--prior Linear-->
  <param3>
    <name>prior_Linear</name>
    <type>int</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
    <valtrans>None</valtrans>
    <valtrans>NotNone</valtrans>
  </param3>
  <!--component -->
  <param4>
    <name>component</name>
    <type>int</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
    <valtrans>None</valtrans>
    <valtrans>int</valtrans>
  </param4>
  <!--store covariance -->
  <param5>
    <name>store_covariance</name>
    <type>bool</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
  </param5>
  <!--tol -->
  <param6>
    <name>tol</name>
    <type>float</type>
    <min>0.0</min>
    <max>1.0</max>
    <def>0.0001</def>
  </param6>
   <!--reg_param -->
  <param7>
    <name>reg_param</name>
    <type>float</type>
    <min>0.0</min>
    <max>1.0</max>
    <def>0.0</def>
  </param7>
  <!--type to generate dataset-->
  <param8>
    <name>type_dataset</name>
    <type>int</type>
    <min>0</min>
    <max>2</max>
    <def>0</def>
  </param8>

</inputs>
