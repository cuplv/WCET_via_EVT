<?xml version="1.0"?>
 <inputs>
  <!--solver -->
  <param0>
    <name>solver</name>
    <type>int</type>
    <min>0</min>
    <max>4</max>
    <def>1</def>
    <valtrans>newton-cg</valtrans>
    <valtrans>lbfgs</valtrans>
    <valtrans>liblinear</valtrans>
    <valtrans>sag</valtrans>
    <valtrans>saga</valtrans>
  </param0>
  <!--penalty -->
  <param1>
    <name>penalty</name>
    <type>int</type>
    <min>0</min>
    <max>4</max>
    <def>1</def>
    <valtrans>l1</valtrans>
    <valtrans>l2</valtrans>
    <valtrans>elasticnet</valtrans>
    <valtrans>none</valtrans>
  </param1>
  <!--dual -->
  <param2>
    <name>dual</name>
    <type>bool</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
  </param2>
  <!--tol -->
  <param3>
    <name>tol</name>
    <type>float</type>
    <min>0.0</min>
    <max>1.0</max>
    <def>0.0001</def>
  </param3>
  <!--C -->
  <param4>
    <name>C</name>
    <type>float</type>
    <min>0.01</min>
    <max>100.0</max>
    <def>10.0</def>
  </param4>
  <!--fit_intercept -->
  <param5>
    <name>fit_intercept</name>
    <type>bool</type>
    <min>0</min>
    <max>1</max>
    <def>0</def>
  </param5>
   <!--intercept_scaling -->
  <param6>
    <name>intercept_scaling</name>
    <type>float</type>
    <min>0.0</min>
    <max>10.0</max>
    <def>1.0</def>
  </param6>
  <!--max_iteration -->
  <param7>
    <name>max_iteration</name>
    <type>int</type>
    <min>95</min>
    <max>105</max>
    <def>100</def>
  </param7>
   <!--multi_class -->
  <param8>
    <name>multi_class</name>
    <type>int</type>
    <min>0</min>
    <max>2</max>
    <def>0</def>
    <valtrans>ovr</valtrans>
    <valtrans>multinomial</valtrans>
    <valtrans>auto</valtrans>
  </param8>
  <!--l1_ratio -->
 <param9>
   <name>l1_ratio</name>
   <type>int</type>
   <min>0</min>
   <max>1</max>
   <def>0</def>
   <valtrans>none</valtrans>
   <valtrans>rand</valtrans>
 </param9>
 <!--class_weight -->
 <param10>
   <name>class_weight</name>
   <type>int</type>
   <min>0</min>
   <max>2</max>
   <def>0</def>
   <valtrans>None</valtrans>
   <valtrans>balanced</valtrans>
   <valtrans>weighted</valtrans>
 </param10>
 <!--random_state-->
 <param11>
   <name>random_state</name>
   <type>int</type>
   <min>0</min>
   <max>1</max>
   <def>0</def>
   <valtrans>None</valtrans>
   <valtrans>int</valtrans>
 </param11>
 <!--verbose-->
 <param12>
   <name>verbose</name>
   <type>int</type>
   <min>0</min>
   <max>1</max>
   <def>0</def>
 </param12>
 <!--warm_start-->
 <param13>
   <name>warm_start</name>
   <type>bool</type>
   <min>0</min>
   <max>1</max>
   <def>0</def>
 </param13>
 <!--n_jobs-->
 <param14>
   <name>n_jobs</name>
   <type>int</type>
   <min>0</min>
   <max>1</max>
   <def>0</def>
   <valtrans>None</valtrans>
   <valtrans>int</valtrans>
 </param14>
</inputs>
